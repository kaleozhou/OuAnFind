<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd"><validators>    <field name="fdtype">        <field-validator type="requiredstring">            <message> required fdtype</message>        </field-validator>        <field-validator type="stringlength">            <param name="minLength">1</param>            <param name="maxLength">256</param>            <message >fdtype should be between ${minLength} and ${maxLength}</message>        </field-validator>    </field>    <field name="fdname">        <field-validator type="requiredstring">            <message> required fdname</message>        </field-validator>        <field-validator type="stringlength">            <param name="minLength">1</param>            <param name="maxLength">512</param>            <message >fdname should be between ${minLength} and ${maxLength}</message>        </field-validator>    </field>    <field name="fdvalue">        <field-validator type="requiredstring">            <message> required fdvalue</message>        </field-validator>        <field-validator type="stringlength">            <param name="minLength">1</param>            <param name="maxLength">2048</param>            <message >fdvalue should be between ${minLength} and ${maxLength}</message>        </field-validator>    </field>    <field name="fdremark">        <field-validator type="stringlength">            <param name="minLength">1</param>            <param name="maxLength">8000</param>            <message >fdremark should be between ${minLength} and ${maxLength}</message>        </field-validator>    </field>    <field name="fdparentid">        <field-validator type="int">            <param name="min">1</param>            <param name="max">1000</param><!-- default setting 1000,you can set the value again -->            <message>fdparentid should be between ${min} and ${max}</message>        </field-validator>    </field>    <field name="fddictionarystatus">        <field-validator type="stringlength">            <param name="minLength">1</param>            <param name="maxLength">256</param>            <message >fddictionarystatus should be between ${minLength} and ${maxLength}</message>        </field-validator>    </field>    <field name="fdoperator">        <field-validator type="stringlength">            <param name="minLength">1</param>            <param name="maxLength">256</param>            <message >fdoperator should be between ${minLength} and ${maxLength}</message>        </field-validator>    </field>    <field name="fdoperatedate">        <field-validator type="date">            <param name="min">1910-01-01</param>            <param name="max">2090-01-01</param><!-- default setting 1000,you can set the value again -->            <message>fdoperatedate should be between ${min} and ${max}</message>        </field-validator>    </field>    <field name="fdstatus">        <field-validator type="int">            <param name="min">1</param>            <param name="max">1000</param><!-- default setting 1000,you can set the value again -->            <message>fdstatus should be between ${min} and ${max}</message>        </field-validator>    </field></validators>
